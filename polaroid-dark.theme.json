        "linkHover": "linkHover",
        "buttonText": "buttonText"
    },
    "gradients": {
        "brand": {
            "direction": "135deg",
            "stops": ["brand1 0%", "brand2 50%", "brand3 100%"]
        },
        "ambient": {
            "angle": "120deg",
            "stops": ["ambient1", "ambient2", "ambient3"]
        },
        "floating": [
            {
                "direction": "135deg",
                "stops": ["ambient1", "ambient2", "ambient3"]
            },
            {
                "direction": "135deg",
                "stops": ["float2_1", "float2_2", "float2_3"]
            },
            {
                "direction": "135deg",
                "stops": ["float3_1", "float3_2", "float3_3"]
            }
        ]
    },
    "shadows": {
        "card": "0 24px 60px rgba(0, 0, 0, 0.5)",
        "cardInner": "inset 0 1px 0 rgba(255, 255, 255, 0.05)",
        "button": "0 12px 28px rgba(0, 0, 0, 0.4)",
