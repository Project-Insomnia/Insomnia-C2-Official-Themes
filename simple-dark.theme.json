            {
                "direction": "135deg",
                "stops": ["float2_1", "float2_2", "float2_3"]
            },
            {
                "direction": "135deg",
                "stops": ["float3_1", "float3_2", "float3_3"]
            }
        ]
    },
    "shadows": {
        "card": "0 8px 30px #0000001F",
        "cardInner": "inset 0 1px 0 #FFFFFF0D",
